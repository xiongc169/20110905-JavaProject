<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.practice.hibernate.domain.Role" table="role" schema="wong_user">
        <id name="id">
            <column name="id" sql-type="bigint(20)"/>
        </id>
        <property name="roleId">
            <column name="role_id" sql-type="bigint(20)"/>
        </property>
        <property name="roleName">
            <column name="role_name" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="timestamp"/>
        </property>
        <property name="modifyTime">
            <column name="modify_time" sql-type="timestamp"/>
        </property>
        <property name="note1">
            <column name="note1" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="note2">
            <column name="note2" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="isDelete">
            <column name="is_delete" sql-type="int(2)"/>
        </property>
    </class>
</hibernate-mapping>